{
    "name": "gyselroth/php-helper-zip",
    "description": "PHP ZIP helpers",
    "keywords": ["helper", "zip"],
    "homepage": "https://gyselroth.com",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "gyselroth",
            "email": "mail@gyselroth.com",
            "homepage": "http://www.gyselroth.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-zip": "*",
        "ext-zlib": "*",
    },
    "require-dev": {
        "phpunit/phpunit": "7.5.16",
        "phpstan/phpstan": "^0.12.4"
    },
    "autoload": {
        "psr-4": {
            "Gyselroth\\Helper\\": "src/Gyselroth/HelperZip/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "secure-http": false,
        "process-timeout" : 0
    },
    "scripts": {
        "test": "phpunit tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        }
    }
}
